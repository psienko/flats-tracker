<h1>Last 100 activities</h1>
<table>
  <thead>
    <tr>
      <td>#</td>
      <td>Event name</td>
      <td>Created at</td>
    </tr>
  </thead>
  <tbody>
    <% @activities.each do |activity| %>
      <tr>
        <td class="home-activity-id"><%= activity.id %></td>
        <td class="home-activity"><%= activity.event_name %></td>
        <td class="home-activity-small"><%= activity.created_at %></td>
      </tr>
    <% end -%>
  </tbody>
</table>
